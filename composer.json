{
    "name": "shaungbhone/contact-form",
    "description": "This package is laravel contact form package with livewire and tailwind",
    "keywords": [
        "shaungbhone",
        "contact-form"
    ],
    "homepage": "https://github.com/shaungbhone/contact-form",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shaung Bhone",
            "email": "shaungbhone.business@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ShaungBhone\\ContactForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShaungBhone\\ContactForm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShaungBhone\\ContactForm\\ContactFormServiceProvider"
            ],
            "aliases": {
                "ContactForm": "ShaungBhone\\ContactForm\\ContactFormFacade"
            }
        }
    }
}
